{
    "name": "prusmarcin/laravel-rest-server",
    "description": "Package Rest Full Server for Laravel 5.6",
    "type": "library",
    "require": {
        "laravel/framework": "^5.6"
    },
	"require-dev": {
        "phpunit/phpunit": "^7.0"
	},
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Prus",
            "email": "mhome@o2.pl"
        }
    ],
	"autoload": {
        "psr-4": {
			"Restserver\\": "src/",
			"Restserver\\Seeds\\": "src/seeds"
        }
    },
	"autoload-dev": {
        "psr-4": {
            "Restserver\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
},
    "minimum-stability": "dev"
}
